												source(request,response){
												$.ajax({
													url:'index.php?route=catalog/manufacturer/autocomplete&user_token={{ user_token }}&filter_name='+encodeURIComponent(request),
													dataType:'json',
													success(json){
													response($.map(json,item=>({label:item.name,value:item.manufacturer_id})));
													}
												});
												},
												select(item){ 

												if ($('#rep-manu-selected-'+row+' input[value="'+item.value+'"]').length){
													return false; 
												}

												$('#rep-manu-selected-'+row).append(
													'<div class="rep-manu-badge">'+item.label+
