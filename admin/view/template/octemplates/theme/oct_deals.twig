<script>
$(document).delegate('[id^=megamenu-] .panel-heading', 'click', function() {
	let id_menu = $(this).parent().attr('id');
	let m_id = id_menu.split('-');

	$.ajax({
		url: 'index.php?route=octemplates/menu/oct_menu/getMenu&user_token={{ user_token }}&menu_id=' + m_id[1],
		type: 'get',
		cache: false,
		dataType: 'html',
		success: function(data) {
			if (data.length > 1) {
				$('#menu_settings-'+ m_id[1]).html(data);
			}
		}
	});

	$(this).siblings('.panel-body').find('.main_menu_block a:first').tab('show');
	$(this).siblings('.panel-body').find('.menu_lang_block a:first').tab('show');
	$(this).siblings('.panel-body').toggle();
});

$(document).delegate('.menu_type', 'change', function() {
	let m_id = $(this).attr("id").split('-');
	let val = $(this).val();

	$.ajax({
		url: 'index.php?route=octemplates/menu/oct_menu/getMenu&user_token={{ user_token }}&type='+ val +'&menu_id=' + m_id[1],
		type: 'get',
		cache: false,
		dataType: 'html',
		success: function(data) {
			if (data.length > 1) {
				$('#menu_settings-'+ m_id[1]).html(data);
			}
		}
	});
});

let menu_id = {{ menu_id }};

function updateSubLinkTitle(subLinkId, title) {
  var headerSpan = $('#sub-link-' + subLinkId + ' .panel-heading span');
  if (title.trim()) {
    headerSpan.html('<i class="fa fa-link" aria-hidden="true"></i> ' + title);
  } else {
    headerSpan.html('<i class="fa fa-link" aria-hidden="true"></i> {{ text_sub_link }}');
  }
}

function addSubLink(menu_id) {
	const sub_link_id = Date.now();
	const config_language_id = '{{ config_language_id }}';
	
	$.ajax({
		url: 'index.php?route=octemplates/menu/oct_menu/getSubLinks&user_token=' + getURLVar('user_token'),
		type: 'get',
		data: {
			menu_id: menu_id,
			sub_link_id: sub_link_id,
			config_language_id: config_language_id
		},
		dataType: 'html',
		success: function(html) {
			$('#sub-links-container-' + menu_id).prepend(html);
			$('#sub-link-' + sub_link_id + ' .nav-tabs li:first-child a').tab('show');
		},
		error: function(xhr, ajaxOptions, thrownError) {
			alert(thrownError + "\r\n" + xhr.statusText + "\r\n" + xhr.responseText);
		}
	});
}

function removeSubLink(sub_link_id) {
	if (confirm('{{ text_confirm_delete }}')) {
		$('#sub-link-' + sub_link_id).remove();
	}
}

