																<script>
																	$('#btn_mass_generate_bought_together').click(function(){
																		var $btn = $(this);
																		$btn.prop('disabled', true).html('<i class="fa fa-spinner fa-spin"></i> {{ text_generating }}');
																		
																		$.ajax({
																			url: '{{ bought_together_cron_url }}',
																			type: 'GET',
																			dataType: 'json',
																			success: function(response){
																				if (response.error) {
																					usNotify('danger', response.error);
																				} else {
																					usNotify('success', '{{ text_generated }}');
																				}
																			},
																			error: function(){
																				usNotify('danger', '{{ text_generated_failed }}');
																			},
																			complete: function(){
																				$btn.prop('disabled', false).html('{{ text_button_generate }}');
																			}
																		});
																	});
																</script>
