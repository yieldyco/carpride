$('input[name=\'advantage_link\']').autocomplete({
	'source': function(request, response) {
		$.ajax({
			url: 'index.php?route=catalog/information/octAutocomplete&user_token={{ user_token }}&filter_name=' +  encodeURIComponent(request),
			dataType: 'json',
			cache: false,
			success: function(json) {
				response($.map(json, function(item) {
					return {
						label: item['name'],
						value: item['information_id'],
						descr: item['description'],
						href: item['href'],
					}
				}));
			}
		});
	},
	'select': function(item) {
		let in_id = $(this).attr('id').split('-');

		$('input[name=\'advantage_link\']').val('');
		$('#advantages_id-' + in_id[1]).val(item.value);

		$.each(item.descr, function(i, val) {
			$('#advantage-title-' + in_id[1] + i).val(val['title']);
		});

		$.each(item.href[{{ store_id }}], function(i, val) {
			$('#advantage-href-' + in_id[1] + i).val('/'+ val);
		});
	}
});

