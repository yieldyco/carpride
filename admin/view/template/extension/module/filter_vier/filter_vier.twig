{% if shabl != 'lic' %}
<script>
    /*setting*/
    function log(str) {console.log(str);}
    
    var setFV = {{ oj_set ? oj_set : '{}' }};
    setFV.save_popup = {{ save_popup ? 'true' : 'false' }};
    setFV.ajax_err = {{ ajax_err ? 'true' : 'false' }};
    setFV.display_block_gen_id = {{ display_block_gen_id ? 'true' : 'false' }};
    setFV.click_select = true;
    setFV.add_url = '';
    setFV.timestamp = null;
    setFV.cls = {
        error_param: 'error_param',
        color_red: 'color_red',
        hiden: 'fv-hiden',
        show: 'fv-show',
        bg_attention: 'bg_attention',
        action_check: 'action_check',
    };
    
    {% if shabl == 'seo_url' %}
    
    setFV.cls.duble_link = 'duble_link';
    setFV.cls.debug_action = 'debug_action';
    
    var row_trans = 0;
    
    $('#get_param').sortable();
    
    $(document).ready(function() {
        
        startDebugInvalTrans();
        
    });
    
    $('.view_duble_links').not('[disabled]').on('click', function() {
        $(this).prop('disabled', true);
        var $elem = $(this), is_checked = $elem.prop('checked'), $parent = $elem.closest('.set_url').find('.fv-list_param'), 
            duble_link = setFV.cls.duble_link || 'duble_link',
            animal_ms = 50, search_hide = 'hide_blok', timestamp_search = null;
        if(is_checked) {$elem.closest('.block_param').addClass(setFV.cls.debug_action);}
        else {$elem.closest('.block_param').removeClass(setFV.cls.debug_action);}
        lockLoadSearch($elem, true);
        if(timestamp_search) {
            clearTimeout(timestamp_search);
        }
        timestamp_search = setTimeout(function() {
            if(is_checked) {
                $parent.find('.fv-param_row').not('.'+duble_link).each(function() {
                    $(this).closest('.fv-param_row').addClass(search_hide).css({'display': 'none'});
                });
            }
            else {
                $parent.find('.fv-param_row.'+search_hide).each(function() {
                    $(this).removeClass(search_hide).css({'display': 'block'});
                });
            }
            if($parent.hasClass('fv-block_attrb')) {
                var $group_block;
                setTimeout(function() {
                    $('.fv-block_attrb').find('.fv-attrbs_group').each(function() {
                        $group_block = $(this);
                        if($group_block.find('.fv-params .fv-param_row').not('.'+search_hide).length) {
                            $group_block.css({'display': 'block'});
                        }
                        else {
                            $group_block.css({'display': 'none'});
                        }
                    }); 
                }, animal_ms / 2);
            }
            lockLoadSearch($elem, false);
        }, 500);
        timestamp_search = setTimeout(function() {
            $elem.removeAttr('disabled');
        }, 1000);
        
    });
    
    $('.view_invalid_translit').not('[disabled]').on('click', function() {
        $(this).prop('disabled', true);
        var $elem = $(this), is_checked = $elem.prop('checked'), $parent = $elem.closest('.set_url').find('.fv-list_param'), 
            cls_attention = setFV.cls.bg_attention, animal_ms = 50, search_hide = 'hide_blok', timestamp_search = null;
        if(is_checked) {$elem.closest('.block_param').addClass(setFV.cls.debug_action);}
        else {$elem.closest('.block_param').removeClass(setFV.cls.debug_action);}
        lockLoadSearch($elem, true);
        if(timestamp_search) {
            clearTimeout(timestamp_search);
        }
        timestamp_search = setTimeout(function() {
            if(is_checked) {
                $parent.find('.transl').not('.'+cls_attention).each(function() {
                    $(this).closest('.fv-param_row').addClass(search_hide).css({'display': 'none'});
                });
            }
            else {
                $parent.find('.fv-param_row.'+search_hide).each(function() {
                    $(this).removeClass(search_hide).css({'display': 'block'});
                });
            }
            if($parent.hasClass('fv-block_attrb')) {
                var $group_block;
                setTimeout(function() {
                    $('.fv-block_attrb').find('.fv-attrbs_group').each(function() {
                        $group_block = $(this);
                        if($group_block.find('.fv-params .fv-param_row').not('.'+search_hide).length) {
                            $group_block.css({'display': 'block'});
                        }
                        else {
                            $group_block.css({'display': 'none'});
                        }
                    }); 
                }, animal_ms / 2);
            }
            lockLoadSearch($elem, false);
            setTimeout(function() {$elem.removeAttr('disabled');}, 1000);
        }, 500);
    });
    
    function lockLoadSearch(elem, lock) {
        if(lock) {
            elem.closest('.view_debug').addClass('blackout').closest('.block_param').find('.fv-list_param').addClass('load_search');
        }
        else {
            elem.closest('.view_debug').removeClass('blackout').closest('.block_param').find('.fv-list_param').removeClass('load_search');
        }
    }
    
    var $get_filter_vier = $("input[name='filter_vier_url_set[get_filter_vier]']");
    fun_hide_show(!$get_filter_vier.prop('checked'),'#chu_filter_vier');
    $get_filter_vier.on('click', function() {
        fun_hide_show(!$get_filter_vier.prop('checked'),'#chu_filter_vier',300);
    });
    
    $('.gen_transl_link').on('click', function() {
        var placeholder, warning_succ = setFV.legnd.succ_gen_transl || 'success !', $parent = $(this).closest('.set_url'), is_set_main = $parent.hasClass('set_main_url');
        loaderImg(true);
        $parent.find('.transl').each(function() {
            placeholder = $(this).attr('placeholder');
            if(placeholder) {
                $(this).val(placeholder);
            }
        });
        if(is_set_main) {resetDebug(true);}
        setDefaultClass($parent);
        setTimeout(function() {loaderImg(false); $('#info_set_url').html(warningPole(warning_succ));}, 300);
    });
    
    $('#record_seo_url').on('click', function() {
        var post_data = {};
        var form_pole = $(':text[name^="transl"]').not('.cls_slider').filter(function() {return this.value;}).serialize();
        var post_data = {"record_seo_url" : form_pole};
        ajs_post(post_data);
    });
    
    $('#clear_duble_links').on('click', function() {
        var obj = {}, error_param = setFV.cls.error_param || 'error_param', color_red = setFV.cls.color_red || 'color_red', 
            duble_link = setFV.cls.duble_link || 'duble_link', 
            warning_succ = setFV.legnd.succ_clear_duble_links || 'success !';
        obj = $('#block_seo_url input.transl.'+color_red+', #block_seo_url input.transl.'+error_param).val('');
        resetDebug(true);
        if(obj[0]) {
            clearPoleAll();
            $('#info_set_url').html(warningPole(warning_succ));
        }
    });
    
    $('.btn_clear_link').on('click', function() {
        var $parent = $(this).closest('.set_url');
        $parent.find('input.transl').val('');
        setDefaultClass($parent);
        $('#info_set_url').empty();
    });
    
    $('#clear_table').on('click', function() {
        var duble_link = setFV.cls.duble_link || 'duble_link';
        showPopupBlok2($(this).text()+' ?');
        $('#action_cancel').on('click', function(){pppBlok(false);});
        $('#action_yes').on('click', function(){
            pppBlok(false);
            var post_data = 'clear_table';
            ajs_post(post_data);
            resetDebug(true);
        });
    });
    
    $('input.transl[placeholder], input.transl_group[placeholder]').on('dblclick', function() {
        var placeholder = $(this).attr('placeholder');
        if(placeholder) {
            $(this).val(placeholder);
        }
    });
    
    $('.view_attrb_slider').on('click', function() {
        var attributs = [], post_data = {}, lang_id = $('[name="filter_vier_url_set[lang_translit]"]').children(':selected').val();
        $(this).closest('.set_url').find('.cls_slider[data-attribute_id]').each(function() {
            attributs.push($(this).data('attribute_id'));
        });
        if(attributs.length) {
            post_data = {"view_attrb_slider" : attributs};
            if(lang_id) {post_data['lang_id'] = lang_id;}
            ajs_post(post_data, true);
        }
    });
    
    function htmlAttrb(view_attrb) {
        /*is ajs_post*/
        var pz = 'attrb', html = '';
        html += '<div class="view_attrb">';
            $.each(view_attrb, function(gr_i, attributes) {
                html += '<div class="fv-attrbs_group">';
                    html += '<div class="fv-group_attrib"><span class="fv-name_group color_white">'+attributes['name_group']+'</span></div>';
                    $.each(attributes[pz], function(attr_i, attribute) {
                        html += '<div class="fv-params">';
                            html += '<div class="fv-param_row param_main"><div class="fv-param_box"><span class="fv-param_name">'+attribute['name_attr']+'</span></div></div>';
                            html += '<div class="fv-params param_value">';
                                $.each(attribute['param'], function(txt_i, val) {
                                    html += '<div class="fv-param_row"><div class="fv-param_box"><span class="fv-param_name" data-text_id="'+val['text_id']+'">'+val['name_text']+'</span></div></div>';
                                });
                            html += '</div>';
                        html += '</div>';
                    });
                html += '</div>';
            });
        html += '</div>';
        return html;
    }
    
    $('.add_group_transl').on('click', function() {
        var transl_group, transl_url;
        $(this).closest('.set_url').find('input.transl_group').each(function() {
            transl_group = $(this).val();
            if(transl_group) {
                transl_group += '-';
                $(this).closest('.set_url').find('.fv-param_row.param_main input.transl').each(function() {
                    transl_url = $(this).val();
                    if(transl_url.indexOf(transl_group) !== 0) {
                        $(this).val(transl_group+transl_url);
                    }
                });
            }
        });
    });
    
    $('#replace_transl').on('click', function() {
        clearPoleAll();
        var post_data = {};
        post_data['replace_transl'] = 1;
        ajs_post(post_data, true);
    });
    
    function saveTransl() {
        var form_pole = $('input[name^="filter_vier_replace_transl"]').serialize();
        var post_data = {"save_transl" : form_pole};
        ajs_post(post_data);
    }
    
    function resetTransl() {
        var post_data = {};
        post_data['reset_transl'] = 1;
        ajs_post(post_data);
    }
    
    $('#modal_window').on('change', 'input[name^="filter_vier_replace_transl"]', function() {
        let search = $(this).val();
        $(this).removeClass('warning_border');
        if(search && search.charAt(0) == ' ') {
            $(this).addClass('warning_border');
        }
    });
    
    $('#modal_window').on('keyup', '.bloc_search_fv input', function() {
		var search_val = $(this).val();
        $('#modal_window .row_blok .view_from input').each(function() {
            let $row_blok = $(this).closest('.row_blok');
            if($(this).val().search(new RegExp(preg_quote(search_val), "gi")) < 0) {
				$row_blok.fadeOut();
			}
            else {
				$row_blok.show();
			}
		});
	});
    
    function startDebugInvalTrans() {
        var placeholder, val_transl, cls_attention = setFV.cls.bg_attention, poles = ['attrb','optv','manufs'];
        $('#block_seo_url').find('input.transl[placeholder]').each(function() {
            val_transl = this.value;
            placeholder = $(this).attr('placeholder');
            if(placeholder != val_transl) {
                $(this).addClass(cls_attention);
            }
        });
        poles.forEach(function(key) {
            if($('.fv-'+key+' .transl').is('.'+cls_attention)) {
                $('.fv-'+key+' .box_invalid_translit').removeClass(setFV.cls.hiden);
            }
        });
    }
    
    function resetDebug(hide_box, pz) {
        var cls_pz = '';
        if(pz) {
           cls_pz = '.fv-'+pz; 
        }
        $('#block_seo_url').find('.block_param.big_block'+cls_pz).each(function() {
            var del_action = false, $check_inva = $(this).find('.box_invalid_translit'), $check_dubl = $(this).find('.box_duble_links');
            if($(this).hasClass(setFV.cls.debug_action)) {
                if($check_inva.find('input').prop('checked')) {
                    if(del_action) {
                        $check_inva.find('input').prop('checked', false);
                    }
                    else {
                        $check_inva.find('input').trigger('click').prop('checked', false);
                        del_action = true;
                    }
                }
                if($check_dubl.find('input').prop('checked')) {
                    if(del_action) {
                        $check_dubl.find('input').prop('checked', false);
                    }
                    else {
                        $check_dubl.find('input').trigger('click').prop('checked', false);
                        del_action = true;
                    }
                }
            }
            if(hide_box) {
                $check_inva.addClass(setFV.cls.hiden);
                $check_dubl.addClass(setFV.cls.hiden);
                $('#block_seo_url '+cls_pz+' .fv-param_row.'+setFV.cls.duble_link).removeClass(setFV.cls.duble_link);
            }
        });
    }
    
    function setDefaultClass($parent) {
        var error_param = setFV.cls.error_param, color_red = setFV.cls.color_red, bg_attention = setFV.cls.bg_attention, duble_link = setFV.cls.duble_link;
        $parent.find('input.transl').removeClass(error_param).removeClass(color_red).removeClass(bg_attention).removeAttr('tabindex');
        if($parent.hasClass('block_settings')) {
            var pz = $parent.data('set_pz');
            resetDebug(true, pz);
        }
        else {
            $parent.find('.fv-param_row.'+duble_link).removeClass(duble_link);
        }
        $('#clear_duble_links').addClass(setFV.cls.hiden);
    }
    
    function setDubleLinks(duble_url) {
        $('#clear_duble_links').removeClass(setFV.cls.hiden);
        var cls = {}, duble_link = setFV.cls.duble_link || 'duble_link', 
            poles = ['attrb','optv','manufs'];
            cls['dbl_fv'] = setFV.cls.error_param || 'error_param';
            cls['dbl_base'] = setFV.cls.color_red || 'color_red';
        poles.forEach(function(key) {$('.fv-'+key+' .box_duble_links').addClass(setFV.cls.hiden);});
        $.each(duble_url, function(err_tabl, duble_urls) {
            $.each(duble_urls, function(pz, arr1) {
                $.each(arr1, function(main_id, arr2) {
                    $.each(arr2, function(key_id, i) {
                        $('.fv-'+pz+' .fv-params .transl[name="transl['+pz+']['+main_id+']['+key_id+']"]').attr('tabindex', 5).addClass(cls[err_tabl]).closest('.fv-param_row').addClass(duble_link);
                    });
                });
            });
        });
        poles.forEach(function(key) {
            if($('.fv-'+key+' .fv-param_row').is('.'+duble_link)) {
                $('.fv-'+key+' .box_duble_links').removeClass(setFV.cls.hiden);
            }
        });
    }
    
    function genTableData(obj) {
        var tabl = '<table id="view_transl" class="max_width"><tbody id="view_pole">';
        $.each(obj,function(from, to) {
            tabl += genViewTransl(from, to);
            row_trans++;
        });
        tabl += '</tbody>';
        tabl += '<tfoot><tr>';
        tabl += '<td class="max_width" colspan="3"></td>';
        tabl += '<td><button type="button" onclick="addPole();" class="btn btn-primary"><i class="fa fa-plus-circle"></i></button></td>';
        tabl += '</tr></tfoot>';
        tabl += '</table>';
        return tabl;
    }
    
    function addPole() {
        let html = genViewTransl('', '');
    	$('#view_pole').append(html);
        let blok_scrol = $('.body_modal.fv-scroll'); blok_scrol.scrollTop(blok_scrol.prop('scrollHeight'));
    	row_trans++;
    }
    
    function genViewTransl(from, to) {
        let html = '<tr class="row_blok" id="pole_row_'+row_trans+'">';
        html += '<td class="view_from"><input name="filter_vier_replace_transl['+row_trans+'][from]" type="text" value="'+from+'" /></td>';
        html += '<td class="strelka"> <span class="fv-icon"><span class="arrow_right"></span></span> </td>';
        html += '<td class="view_to"><input name="filter_vier_replace_transl['+row_trans+'][to]" type="text" value="'+to+'" /></td>';
        html += '<td><button type="button" onclick="$(\'#pole_row_'+row_trans+'\').remove();" class="btn btn-danger"><i class="fa fa-minus-circle"></i></button></td>';
        html += '</tr>';
        return html;
    }
    
