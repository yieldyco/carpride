                    <input form="form_post" type="checkbox" name="filter_vier_url_set[{{ pole }}]" value="1" {{ chek }} />
                    {% set pole = 'gen_transl' %}
                    <button form="form_post" type="submit" name="{{ pole }}" id="{{ pole }}" data-toggle="tooltip" title="{{ arr_langs['help'][pole] }}" class="button_submit bg_grey">{{ arr_langs['legnd'][pole] }}</button>
                </span>
                {% set pole = 'gen_transl' %}
                <span data-toggle="tooltip" title="{{ arr_langs['help'][pole] }}" class="gen_transl_link button_submit bg_grey">{{ arr_langs['legnd'][pole] }}</span>
                <span class="fv-icon"><span class="arrow_right"></span></span>
                {% set pole = 'record_seo_url' %}
                <span data-toggle="tooltip" title="{{ arr_langs['help'][pole] }}" id="{{ pole }}" class="button_submit bg_grey">{{ arr_langs['legnd'][pole] }}</span>
                <span class="fv-icon"><span class="arrow_left"></span></span>
                <span id="clear_duble_links" class="fv-hiden">
                    {% set pole = 'clear_duble_links' %}
                    <span data-toggle="tooltip" title="{{ arr_langs['help'][pole] }}" class="button_submit bg_white color_cadetblue">{{ arr_langs['legnd'][pole] }}</span>
                </span>
            </div>          
            <div class="sett_inline_flex pole_posit_right">
                {% set pole = 'replace_transl' %}
                <span data-toggle="tooltip" title="{{ arr_langs['help'][pole] }}" id="{{ pole }}" class="button_submit bg_grey">{{ arr_langs['legnd'][pole] }}</span>
                {% if attrb_slider %}
                    {% set pole = 'view_attrb_slider' %}
                    <span data-toggle="tooltip" title="{{ arr_langs['help'][pole] }}" class="{{ pole }} button_submit bg_blue">{{ arr_langs['legnd'][pole] }}</span>
                {% endif %}
                {% set pole = 'clear_table' %}
                <span data-toggle="tooltip" title="{{ arr_langs['help'][pole] }}" id="{{ pole }}" class="button_submit bg_red">{{ arr_langs['legnd'][pole] }}</span>
            </div>
        </div></div>
        <div class="name_group_main">{{ arr_langs['help']['edit_transl'] }}</div>
            <div class="fv-wrapper" data-count_blocks="{{ count_blocks }}">
    <!-- gen-seo: 'prs','psp','nows','qnts' -->
    {% if one_block_seo_url %}
    <div class="one_block">
        {% for pz in one_block_seo_url %}
            {% set main_id = 1 %}
    <div class="block_param fv-{{ pz }}">
        <div class="block_settings set_url">
            <div class="title_block block_flex">
                <span class="name_block_set">{{ arr_langs['legnd'][pz] }}</span>
