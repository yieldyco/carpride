    <div class="block_param big_block fv-{{ pz }}">
        <div class="block_settings set_url" data-set_pz="{{ pz }}">
            <div class="title_block block_flex">
                <span class="name_block_set">{{ arr_langs['legnd'][pz] }}</span>
