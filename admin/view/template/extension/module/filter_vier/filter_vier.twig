        {% for key, val in tab_nav %}
            {% set tab_status = 'tab_link tab_base' %}
            {% if val['id'] in arr_tab %}
                {% set tab_status = 'tab_link' %}
            {% endif %}
            {% if shabl == val['id'] %}
                {% set tab_status = tab_status ~ ' active' %}
            {% endif %}
            <li id="{{ val['id'] }}" class="{{ tab_status }}" data-tab_id="{{ key }}"><a href="#{{ val['href'] }}" data-toggle="tab">{{ val['txt'] }}</a></li>
        {% endfor %}
        </ul>
    <div class="tab-content form-horizontal">
    
    {% if shabl == 'seo_url' %}
    
    <div id="tab-{{ shabl }}">
            <div class="form_group group_set"><div class="block_flex">
                <div class="sett_inline_flex">
                    {% set pole = 'seo_url' %}
