                    <input class="input_width_100" type="text" name="filter_vier_url_set[{{ pole }}]" value="{{ filter_vier_url_set[pole] }}" placeholder="minus" />
                </div>
                </span>
                <div class="sett_inline_flex">
                    {% set pole = 'get_filter_vier' %}
                    <span class="for_label helpis" data-toggle="tooltip" title="{{ arr_langs['help'][pole] }}">{{ arr_langs['legnd'][pole] }}</span>
                    {% set chek = null %} {% if filter_vier_url_set[pole] %} {% set chek = 'checked="checked"' %} {% endif %}
                    <input class="set_default" type="checkbox" name="filter_vier_url_set[{{ pole }}]" value="1" {{ chek }} />
                </div>
                {% set pole = 'chu_filter_vier' %}
                <div id="{{ pole }}">
                    <div class="sett_inline_flex">
                        <span class="for_label helpis" data-toggle="tooltip" title="{{ arr_langs['help'][pole] }}">{{ arr_langs['legnd'][pole] }}</span>
                        <input class="input_width_100 set_default" type="text" name="filter_vier_url_set[{{ pole }}]" value="{{ filter_vier_url_set[pole] }}" placeholder="fvs" />
                    </div>
                </div>
                {% set arr_poles = ['no_seo_url_page_search','http_server'] %}
                {% for pole in arr_poles %}
                <div class="sett_inline_flex">
                    <span class="for_label helpis" data-toggle="tooltip" title="{{ arr_langs['help'][pole] }}">{{ arr_langs['legnd'][pole] }}</span>
                    {% set chek = null %} {% if filter_vier_url_set[pole] %} {% set chek = 'checked="checked"' %} {% endif %}
                    <input type="checkbox" name="filter_vier_url_set[{{ pole }}]" value="1" {{ chek }} />
                </div>
                {% endfor %}
            </div></div>
            <div class="form_group group_set"><div class="block_flex">
                <div class="sett_inline_flex">
                    {% set pole = 'sort_url' %}
                    <span class="for_label helpis" data-toggle="tooltip" title="{{ arr_langs['help'][pole] }}">{{ arr_langs['legnd'][pole] }}</span>
                    {% set chek = null %} {% if filter_vier_url_set[pole] %} {% set chek = 'checked="checked"' %} {% endif %}
                    <input type="checkbox" name="filter_vier_url_set[{{ pole }}]" value="1" {{ chek }} />
                </div>
                <div class="sett_inline_flex">
                    {% set pole = 'sort_get_param' %}
                    {% if filter_vier_url_set[pole] %}
                        <div id="get_param" class="ram_dash">
                        {% for pz in filter_vier_url_set[pole] %}
                            <span class="get_param">{{ arr_langs['legnd'][pz] }}
                                <input type="hidden" name="filter_vier_url_set[{{ pole }}][]" value="{{ pz }}" />
                            </span>
                        {% endfor %}
                        </div>
                    {% endif %}
                </div>
                <div class="sett_inline_flex">
                    {% set pole = 'sort_url_in_param' %}
                    <span class="for_label helpis" data-toggle="tooltip" title="{{ arr_langs['help'][pole] }}">{{ arr_langs['legnd'][pole] }}</span>
                    {% set chek = null %} {% if filter_vier_url_set[pole] %} {% set chek = 'checked="checked"' %} {% endif %}
                    <input type="checkbox" name="filter_vier_url_set[{{ pole }}]" value="1" {{ chek }} />
                </div>
            </div></div>
    <div id="block_seo_url" class="set_url set_main_url"><!-- block_seo_url -->
        <span tabindex="0"></span>
        <div id="info_set_url" class="info_set_url"></div>
        <div class="form_group"><div class="block_flex">
            <div class="sett_inline_flex pole_group">
                <span class="setting_admin" style="display: none;">
                    {% set pole = 'del_tire' %}
                    <span class="for_label helpis" data-toggle="tooltip" title="{{ arr_langs['help'][pole] }}">{{ arr_langs['legnd'][pole] }}</span>
