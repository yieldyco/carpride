      <div class="panel-heading panel_infa">
        <h3 class="panel-title"><i class="fa fa-pencil"></i> {{ text_edit }}</h3>
            <div id="ssuu">
            {% if success %}
            <div class="seo_infa alert alert-success"><i class="fa fa-check-circle"></i> {{ success }}
                <button type="button" class="close" data-dismiss="alert">&nbsp;&times;</button>
            </div>
            {% endif %}
            </div>
      </div>
      <!-- start module -->
    <div id="fv-module" class="panel-body fv-module div_form shabl-{{ shabl }}">
    <form action="{{ action }}" method="post" enctype="multipart/form-data" id="form_post">
