                            <span class="fv-param_name">{{ main_translit[pz]['name'] }}</span>
                            <span class="input_translit">
                                <input class="transl" tabindex="" name="transl[{{ pz }}][{{ main_id }}][0]" type="text" value="{{ main_translit[pz]['translit'] }}" placeholder="{{ main_translit[pz]['placeholder'] }}" />
