    $('#gen_text_id').on('click', function() {
        clearPoleAll(); var attributs = [], count_attrbs, name_mod = 'filter_vier', cls_yes_gen = 'yes_gen', cls_no_gen = 'no_gen';
        $('.fv-attrb .fv-param_row').removeClass(cls_yes_gen).removeClass(cls_no_gen);
        $(".fv-attrb .fv-param_row").not('.'+search_hide).find($("input[name='"+name_mod+"_attribs[attrb][view][]']:checked")).each(function() {
            attributs.push($(this).val());
        });
        count_attrbs = attributs.length;
        if(count_attrbs) {
            var post_data = '', post_data_dop = '', end_row = false, val_start = 1, 
                separ = $("input[name='"+name_mod+"_setting[attrb][separ]']").val(),
                del_html_tag = $("input[name='"+name_mod+"_setting[attrb][del_html_tag]']:checked").val(),
                add_rec = $("input[name='"+name_mod+"_setting[attrb][add_rec]']:checked").val(),
                chunk = $("input[name='"+name_mod+"_setting[attrb][chunk]']:checked").val();
            if(separ) {post_data_dop += '&separ='+JSON.stringify(separ);}
            if(del_html_tag) {post_data_dop += '&del_html_tag='+del_html_tag;}
            if(add_rec) {post_data_dop += '&add_rec='+add_rec;}
            if(chunk) {
                var text_result = '', key;
                loaderImg(true);
                var request = function(i) {
                    if(i < count_attrbs) {
                        key = attributs[i]; 
                        post_data = 'gen_text_id='+JSON.stringify([key]);
                        post_data += '&chunk=1';
                        if((count_attrbs - 1) == i) {end_row = true; post_data += '&end_row=1';}
                        if((0 == i)) {
                            post_data += '&start='+val_start;
                        }
                        post_data += post_data_dop;
                        $.ajax({
                            type: 'POST',
                            dataType: 'json',
                            url: setFV.ajax_url_post,
                            data: post_data,
                        }).done(function(data) {
                            if(data.error_warning) {
                                $('#error_warning').append(warningPole(data.error_warning));
                                $('html, body').animate({scrollTop: $('#error_warning').offset().top-20}, 'slow');
                                /*control*/loaderImg(false);
                            }
                            if(data.succ) {
                                text_result = data.succ;
                                if(data.logs_attribs) {$('#block_log').append(warningPole(data.logs_attribs));}
                                if(data.err_attribs) {
                                    if(data.err_attribs[key]) {
                                        $('#attrb_id_'+key).addClass(cls_no_gen);
                                    }
                                } else {
                                    $('#ssuu').html(successPole('attr: '+key+' - '+text_result));
                                    $('#attrb_id_'+key).addClass(cls_yes_gen);
                                }
                                request(i+1);
                            }
                            else {
                                if(data[0]) {
                                    $('#error_warning').append(errorPole(data));
                                    $('html, body').animate({scrollTop: $('#error_warning').offset().top-20}, 'slow');
                                    /*control*/loaderImg(false);
                                }
                            }
                        }).fail(function(err) {
                            if(setFV.ajax_err) {
                                var text_error = errorAjax(err);
                                $('#error_warning').html(errorPole('ERROR!!! '+text_error));
                            }
                            /*control*/loaderImg(false);
                        });
                    }
                    else {
                        loaderImg(false);
                        if(end_row) {
                            if($('#block_log').children().length) {
                                $('#ssuu .seo_infa').append(' >><span id="logs_attribs_err">'+setFV.warning_logs+'</span><< ');
                            }
                        }
                    }
                };
                request(0);
                /*control*//*loaderImg(false);*/
            }
            else {
                post_data = 'gen_text_id='+JSON.stringify(attributs);
                post_data += post_data_dop;
                post_data += '&start='+val_start;
                post_data += '&end_row=1';
                ajs_post(post_data);
            }
            /*control*//*loaderImg(false);*/
        }
        else {
            $('#ssuu').html(warningPole(setFV.warning_no_attribs));
        }
    });
    
