    function ajs_post(post_data, view_info) {
        if(view_info === undefined) {view_info = false;}
        if(!view_info) {clearPoleAll();}
        $.ajax({
            type: 'POST',
            dataType: 'json',
            url: setFV.ajax_url_post,
            data: post_data,
            beforeSend: function(){
                if(!view_info) {loaderImg(true);}
            }
        }).done(function(data) {
            if(data.clear_table_seo_url) {
                $('#block_seo_url').find('input.transl').val('');
                if(data.succ) {
                    $('#info_set_url').html(successPole(data.succ));
                    $('#ssuu').html(successPole(data.succ));
                }
            }
            else if(data.error_warning || data.warning_seo_url) {
                if(data.error_warning) {
                    $('#error_warning').html(errorPole(data.error_warning));
                }
                if(data.warning_seo_url) {
                    $('#info_set_url').html(errorPole(data.warning_seo_url));
                }
                if(data.duble_hand_links) {
                    var it = 1;
                    $.each(data.duble_hand_links,function(key, data) {
                        $("#id_"+key).addClass(setFV.cls.error_param).attr('tabindex', ++it);
                    });
                }
                if(data.error_class_separators) {
                    $("#error_class_separators").addClass(setFV.cls.error_param);
                }
                if(data.dubl_chu_filter_vier) {
                    $('#chu_filter_vier').addClass(setFV.cls.error_param);
                }
                if(data.duble_url) {
                    setDubleLinks(data.duble_url);
                }
            }
            else if(data.view_attrb_slider) {
                let head_text = data.head_text ? data.head_text : 'Sliders', body_text = '', foter_left = '', foter_center = '';
                    body_text = htmlAttrb(data.show_attrb_slider);
                showModalWindow(head_text, body_text, foter_left, foter_center, 'modal_view_param');
            }
            else if(data.replace_transl) {
                row_trans = 0;
                let head_text = data.head_text ? data.head_text : '', body_text = '', placeholder_from = '',
                    foter_left = '<span class="button_submit bg_grey" onclick="resetTransl();">'+setFV.button_default+'</span>',
                    foter_center = '<span class="button_submit bg_grey" onclick="saveTransl();">'+setFV.button_save+'</span>';
                    head_text += data.help_from_to ? data.help_from_to : '';
                    placeholder_from = data.placeholder_from ? data.placeholder_from : '';
                    head_text += '<div class="bloc_search_fv"><input class="form-control" type="text" placeholder="'+placeholder_from+'" /></div>';
                    body_text = genTableData(data.replace_transl);
                showModalWindow(head_text, body_text, foter_left, foter_center, 'transl_modal');
            }
            else if(data.icon_awesome) {
                let head_text = '', body_text = data.icon_awesome, text_clear = data.text_clear ? data.text_clear : '',
                    foter_left = '<span class="clear_icon vibor_icon"><i class=""></i></span>'+text_clear,
                    foter_center = '';
                showModalWindow(head_text, body_text, foter_left, foter_center, 'icon_modal');
            }
            else if(data.info_text) {
                if(data.succ) {
                    $('#info_text').empty().html(successText(data.succ));
                }
                else if(data.duble_alias_base) {
                    var id = data.hl_id || 0, text_info = data.duble_alias_base+' <span onclick="add_hand_links('+id+', 1)" class="ignore_link btn_css">'+data.ignore_duble+'</span>';
                    $('#info_text').empty().html(errorText(text_info));
                }
                else if(data.warning) {
                    $('#info_text').empty().html(errorText(data.warning));
                }
                else {
                    $('#info_text').empty().html(errorText('error'));
                }
            }
            else if(data.succ) {
                $('#ssuu').html(successPole(data.succ));
                if(data.save_ajax) {
                    if(setFV.timestamp) {
                        clearTimeout(setFV.timestamp);
                    }
                    setFV.timestamp = setTimeout(function() {$('#ssuu').empty();}, 2000);
                }
                if(data.logs_attribs) {$('#block_log').append(warningPole(data.logs_attribs));}
                if(data.info_set_url) {$('#info_set_url').html(successPole(data.info_set_url));resetDebug(true);}
            }
            if(data.redir) {
                redir_tab(data.redir);
            }
            else if(data.redir_time) {
                setTimeout(function(){
                    redir_tab(data.redir_time);
                },400);
            }
            else if(data.blok_link_id) {
                if(data.error_link) {
                    $('#'+data.blok_link_id+' .block_view_link').empty().html('<span class="'+setFV.cls.color_red+'">'+data.error_link+'</span>');
                }
                else if(data.yes_link) {
                    $('#'+data.blok_link_id+' .block_view_link').empty().html('<span class="link_view"><a href="'+data.yes_link+'" target="_blank">'+data.text_link+' <i class="fa fa-external-link"></i></a></span>');
                }
            }
            else {
                if(data[0]) {$('#error_warning').append(errorPole(data));$('html, body').animate({scrollTop: $('#error_warning').offset().top-20}, 'slow');}
            }
        }).then(function() {
            if(!view_info) {loaderImg(false);}
        }).fail(function(err) {
            if(setFV.ajax_err) {
                var text_error = errorAjax(err);
                $('#error_warning').html(errorPole('ERROR!!! '+text_error));
            }
            /*control*/loaderImg(false);
        });
    }
    
