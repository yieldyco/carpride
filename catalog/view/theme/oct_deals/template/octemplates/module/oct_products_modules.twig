{% if products %}
    {% if position == 'column_left' or position == 'column_right' %}
        <div class="content-block overflow-hidden position-relative">
            <div class="ds-column-title pb-3 mb-3 fw-500 dark-text">
                {% if link is defined and link %}
                    <a href="{{ link }}">
                        {{ heading_title }}
                    </a>
                {% else %}
                    {{ heading_title }}
                {% endif %}
            </div>
            <div id="ds-column-{{ module_name }}_{{ module }}" class="ds-column-products swiper">
                <div class="swiper-wrapper">
                    {% for product in products %}
                        <div class="ds-module-item swiper-slide">
                            <div class="h-100 d-flex flex-column{% if product.oct_grayscale %} ds-no-stock{% endif %}">
                                <div class="ds-module-img d-flex flex-column">
                                    <div class="ds-module-img-box position-relative">
                                        <div class="ds-module-stickers d-flex flex-wrap">
                                            {% if product.oct_stickers is not empty %}
                                                {% for key, oct_sticker in product.oct_stickers %}
                                                    {% if oct_sticker is not empty and oct_sticker is not iterable %}
                                                        <div class="ds-module-sticker br-12 fw-500 ds-module-sticker-{{ key }}">
                                                            {{ oct_sticker }}
                                                        </div>
                                                    {% endif %}
                                                {% endfor %}
                                            {% endif %}
                                        </div>
                                        <div class="ds-module-buttons position-absolute d-flex flex-column br-8">
                                            {% if oct_popup_view_status %}
                                            <button type="button" aria-label="Quick view" class="ds-module-button ds-module-button-viewed align-self-stretch p-0" title="{{ oct_popup_view }}" onclick="octPopUpView('{{ product.product_id }}')">
                                                <svg width="15" height="11" viewBox="0 0 15 11" fill="none" xmlns="http://www.w3.org/2000/svg">
                                                    <path d="M14.6037 4.17854C13.5937 2.61796 11.3268 0 7.5 0C3.67322 0 1.40629 2.61796 0.396332 4.17854C-0.132111 4.99325 -0.132111 6.00604 0.396332 6.82146C1.40629 8.38204 3.67322 11 7.5 11C11.3268 11 13.5937 8.38204 14.6037 6.82146C15.1321 6.00604 15.1321 4.99396 14.6037 4.17854ZM13.6136 6.275C12.729 7.64184 10.7576 9.93548 7.5 9.93548C4.24243 9.93548 2.27096 7.64255 1.38638 6.275C1.07716 5.79667 1.07716 5.20263 1.38638 4.72431C2.27096 3.35747 4.24243 1.06382 7.5 1.06382C10.7576 1.06382 12.729 3.35676 13.6136 4.72431C13.9236 5.20334 13.9236 5.79667 13.6136 6.275ZM7.5 2.48387C5.69699 2.48387 4.23089 3.83723 4.23089 5.5C4.23089 7.16277 5.69699 8.51613 7.5 8.51613C9.30301 8.51613 10.7691 7.16277 10.7691 5.5C10.7691 3.83723 9.30301 2.48387 7.5 2.48387ZM7.5 7.45161C6.33312 7.45161 5.38469 6.57658 5.38469 5.5C5.38469 4.42342 6.33312 3.54839 7.5 3.54839C8.66688 3.54839 9.61531 4.42342 9.61531 5.5C9.61531 6.57658 8.66688 7.45161 7.5 7.45161Z"
                                                        fill="#00171F" />
                                                </svg>
                                            </button>
                                            {% endif %}
                                            <button type="button" aria-label="Wishlist" class="ds-module-button ds-module-button-wishlist align-self-stretch p-0 ds-wishlist-btn" title="{{ button_wishlist }}" onclick="wishlist.add('{{ product.product_id }}');">
                                                <svg width="15" height="13" viewBox="0 0 15 13" fill="none" xmlns="http://www.w3.org/2000/svg">
                                                    <g>
                                                        <path d="M7.49998 12.5C7.42931 12.5 7.35862 12.4853 7.29262 12.4553C7.07462 12.3559 1.94398 9.97662 1.12132 5.73929C0.803317 4.09996 1.12265 2.50062 1.97532 1.46195C2.66532 0.620615 3.65729 0.173271 4.84462 0.167271C4.85062 0.167271 4.85662 0.167271 4.86196 0.167271C6.21662 0.167271 7.04266 0.938623 7.49933 1.59529C7.95799 0.935957 8.79062 0.161271 10.1539 0.167271C11.342 0.173271 12.3346 0.620615 13.0253 1.46195C13.8766 2.49995 14.1953 4.09928 13.8766 5.73994C13.0553 9.97728 7.92395 12.3573 7.70595 12.456C7.64129 12.4853 7.57065 12.5 7.49998 12.5ZM4.86131 1.16662C4.85731 1.16662 4.854 1.16662 4.85 1.16662C3.95799 1.17062 3.25134 1.48327 2.74868 2.09594C2.08268 2.90727 1.842 4.19795 2.10333 5.54862C2.74 8.83128 6.56198 10.9646 7.49998 11.4433C8.43798 10.9646 12.26 8.83128 12.896 5.54862C13.1586 4.19728 12.918 2.90661 12.2533 2.09594C11.7506 1.48394 11.044 1.17194 10.15 1.16727C10.146 1.16727 10.142 1.16727 10.1387 1.16727C8.55732 1.16727 7.99669 2.75196 7.97402 2.81929C7.90469 3.02129 7.71396 3.1586 7.50063 3.1586C7.4993 3.1586 7.49861 3.1586 7.49794 3.1586C7.28394 3.15794 7.09329 3.02128 7.02529 2.81795C7.00329 2.75128 6.44197 1.16662 4.86131 1.16662Z"
                                                            fill="#00171F" />
                                                    </g>
                                                </svg>
                                            </button>
                                            <button type="button" aria-label="Compare" class="ds-module-button ds-module-button-compare align-self-stretch p-0 ds-compare-btn" title="{{ button_compare }}" onclick="compare.add('{{ product.product_id }}');">
                                                <svg width="15" height="14" viewBox="0 0 15 14" fill="none" xmlns="http://www.w3.org/2000/svg">
                                                    <g>
                                                        <path
                                                            d="M14 7.0001V9.0001C14 10.3788 12.878 11.5001 11.5 11.5001H2.70736L3.854 12.6467C4.04934 12.8421 4.04934 13.1588 3.854 13.3541C3.75667 13.4514 3.62865 13.5007 3.50065 13.5007C3.37265 13.5007 3.24463 13.4521 3.1473 13.3541L1.1473 11.3541C1.1013 11.3081 1.06472 11.2528 1.03939 11.1915C0.988721 11.0695 0.988721 10.9315 1.03939 10.8095C1.06472 10.7482 1.1013 10.6927 1.1473 10.6467L3.1473 8.64674C3.34263 8.45141 3.65932 8.45141 3.85466 8.64674C4.04999 8.84208 4.04999 9.15877 3.85466 9.3541L2.70801 10.5007H11.5C12.3267 10.5007 13 9.82808 13 9.00075V7.00075C13 6.72475 13.224 6.50075 13.5 6.50075C13.776 6.50075 14 6.7241 14 7.0001ZM1.5 7.5001C1.776 7.5001 2 7.2761 2 7.0001V5.0001C2 4.17276 2.67333 3.5001 3.5 3.5001H12.2926L11.146 4.64674C10.9507 4.84208 10.9507 5.15877 11.146 5.3541C11.2433 5.45143 11.3713 5.50075 11.4993 5.50075C11.6273 5.50075 11.7554 5.4521 11.8527 5.3541L13.8527 3.3541C13.8987 3.3081 13.9353 3.25284 13.9606 3.1915C14.0113 3.0695 14.0113 2.9315 13.9606 2.8095C13.9353 2.74817 13.8987 2.69274 13.8527 2.64674L11.8527 0.646744C11.6574 0.451411 11.3407 0.451411 11.1453 0.646744C10.95 0.842077 10.95 1.15877 11.1453 1.3541L12.292 2.50075H3.5C2.122 2.50075 1 3.62208 1 5.00075V7.00075C1 7.27608 1.224 7.5001 1.5 7.5001Z"
                                                            fill="#00171F" />
                                                    </g>
                                                </svg>
                                            </button>
                                        </div>
                                        <a href="{{ product.href }}" class="d-block mx-auto">
                                            <img src="{{ product.thumb }}" class="d-block mx-auto" alt="{{ product.name }}" title="{{ product.name }}" width="{{ product.width }}" height="{{ product.height }}" loading="lazy" />
                                        </a>
                                    </div>
                                </div>
                                <div class="ds-module-caption d-flex flex-column h-100">
                                    <a href="{{ product.href }}" class="ds-module-title fw-500 dark-text">{{ product.name }}</a>
                                    {% if product.oct_stickers is not empty %}
                                        <div class="ds-module-stickers ds-module-sticker-images d-flex align-items-center">
                                            {% for key, oct_sticker in product.oct_stickers %}
                                                {% if oct_sticker is not empty and oct_sticker is iterable %}
                                                        <span class="ds-module-sticker-image"><img src="{{ oct_sticker.image }}" alt="{{ oct_sticker.title }}" data-bs-html="true" data-bs-placement="top" data-bs-toggle="popover" data-bs-trigger="hover" title="{{ oct_sticker.title }}" data-bs-content="{{ oct_sticker.description }}" width="20" height="20" loading="lazy"/></span>
                                                {% endif %}
                                            {% endfor %}
                                        </div>
                                    {% endif %}
                                    {% if product.oct_model is defined and product.oct_model %}
                                        <div class="ds-module-code light-text">{{ oct_view_model_cat }} {{ product.oct_model }}</div>
                                    {% endif %}
                                    {% if product.quantity %}
                                        <div class="ds-module-stock green-text">{{ oct_product_stock }}</div>
                                    {% endif %}
                                    {% if product.rating is not same as(false) %}
                                        <div class="ds-module-rating d-flex align-items-center">
                                            <div class="ds-module-rating-stars d-flex align-items-center me-2" data-rating="{{ product.rating }}">
                                                {% for i in 1..5 %}
                                                    <span class="ds-module-rating-star"><span class="ds-module-rating-star-inner"></span></span>
                                                {% endfor %}
                                            </div>
                                            <div class="ds-module-reviews d-flex align-items-center">
                                                <svg width="13" height="12" viewBox="0 0 13 12" fill="none" xmlns="http://www.w3.org/2000/svg">
                                                    <g>
                                                        <path
                                                            d="M1.88345 11.5383C1.82835 11.5383 1.77318 11.5276 1.72036 11.506C1.5613 11.4401 1.45741 11.2844 1.45741 11.1123V2.5916C1.45741 1.21806 2.21405 0.461426 3.58759 0.461426H10.4042C11.7777 0.461426 12.5343 1.21806 12.5343 2.5916V7.70403C12.5343 9.07757 11.7777 9.83421 10.4042 9.83421H3.76427L2.18508 11.4134C2.10329 11.4952 1.99422 11.5383 1.88345 11.5383ZM3.58759 1.3135C2.69178 1.3135 2.30948 1.69579 2.30948 2.5916V10.0836L3.28651 9.10653C3.3666 9.02644 3.47455 8.98158 3.58759 8.98158H10.4042C11.3 8.98158 11.6823 8.59929 11.6823 7.70347V2.59105C11.6823 1.69524 11.3 1.31294 10.4042 1.31294H3.58759V1.3135ZM9.6941 4.01172C9.6941 3.77655 9.50323 3.58569 9.26806 3.58569H4.72369C4.48851 3.58569 4.29765 3.77655 4.29765 4.01172C4.29765 4.24689 4.48851 4.43776 4.72369 4.43776H9.26806C9.50323 4.43776 9.6941 4.24689 9.6941 4.01172ZM7.98996 6.28391C7.98996 6.04874 7.79909 5.85788 7.56392 5.85788H4.72369C4.48851 5.85788 4.29765 6.04874 4.29765 6.28391C4.29765 6.51908 4.48851 6.70995 4.72369 6.70995H7.56392C7.79909 6.70995 7.98996 6.51908 7.98996 6.28391Z"
                                                            fill="#9CA3AF" />
                                                    </g>
                                                </svg>
                                                <span class="dark-text fsz-10">{{ product.reviews }}</span>
                                            </div>
                                        </div>
                                    {% endif %}
                                    {% if product.price %}
                                    <div class="ds-module-price mt-auto">
                                        {% if product.tax %}
                                        <div class="price-tax fw-300 fsz-12 light-text mb-1">
                                            {{ text_tax }} {{ product.tax }}
                                        </div>
                                        {% endif %}
                                        {% if not product.special %}
                                            <div class="ds-price-new fsz-20 fw-700 dark-text">{{ product.price }}</div>
                                        {% else %}
                                            <div class="d-flex align-items-center">
                                                <div class="ds-price-old fsz-12 light-text text-decoration-line-through fw-500">{{ product.price }}</div>
                                                {% if oct_sticker_you_save and product.you_save %}
                                                    <div class="ds-module-sticker br-12 fw-500 red-bg ms-2">{{ product.you_save }}</div>
                                                {% endif %}
                                            </div>
                                            <div class="ds-price-new fsz-20 fw-700 red-text">{{ product.special }}</div>
                                        {% endif %}
                                    </div>
                                    {% endif %}
                                    <div class="ds-module-cart d-flex align-items-center justify-content-between{% if not product.quantity_show %} position-relative{% endif %}">
                                        {% if product.can_buy is not empty and product.can_buy %}
                                            {% if product.can_buy or not product.quantity %}
                                            <div class="ds-module-quantity{% if quantity_show or product.quantity_show is defined and product.quantity_show %} d-flex{% else %} d-none{% endif %} align-items-center justify-content-center br-8">
                                                <button type="button" aria-label="Minus" class="ds-module-quantity-btn d-flex align-items-center justify-content-center ds-minus">
                                                    <svg width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg">
                                                        <g>
                                                            <path
                                                                d="M3.33333 7.5L12.6667 7.5C12.9427 7.5 13.1667 7.724 13.1667 8C13.1667 8.276 12.9427 8.5 12.6667 8.5L3.33333 8.5C3.05733 8.5 2.83333 8.276 2.83333 8C2.83333 7.724 3.05733 7.5 3.33333 7.5Z"
                                                                fill="#25314C" />
                                                        </g>
                                                    </svg>
                                                </button>
                                                <input type="text" class="form-control" name="quantity" value="{% if product.minimum %}{{ product.minimum }}{% else %}1{% endif %}" aria-label="Quantity" inputmode="numeric">
                                                <button type="button" aria-label="Plus" class="ds-module-quantity-btn d-flex align-items-center justify-content-center ds-plus">
                                                    <svg width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg">
                                                        <g>
                                                            <path
                                                                d="M2.83341 8.00009C2.83341 7.72409 3.05741 7.50009 3.33341 7.50009L7.50008 7.50009L7.50008 3.33342C7.50008 3.05742 7.72408 2.83342 8.00008 2.83342C8.27608 2.83342 8.50008 3.05742 8.50008 3.33342L8.50008 7.50009L12.6667 7.50008C12.9427 7.50008 13.1667 7.72408 13.1667 8.00008C13.1667 8.27608 12.9427 8.50008 12.6667 8.50008L8.50008 8.50009L8.50008 12.6668C8.50008 12.9428 8.27608 13.1668 8.00008 13.1668C7.72408 13.1668 7.50008 12.9428 7.50008 12.6668L7.50008 8.50009L3.33342 8.50009C3.05742 8.50009 2.83342 8.27609 2.83341 8.00009Z"
                                                                fill="#25314C" />
                                                        </g>
                                                    </svg>
                                                </button>
                                            </div>
                                            {% endif %}
                                            {% if quantity_show or product.quantity_show is defined and product.quantity_show %}
                                                <button type="button" aria-label="To cart" class="button button-outline button-outline-primary br-8 fsz-12 ds-module-cart-btn">
                                                    <svg width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg">
                                                        <path
                                                            d="M13.3333 4.10256H11.4872V3.48718C11.4872 1.5639 9.92328 0 8 0C6.07672 0 4.51282 1.5639 4.51282 3.48718V4.10256H2.66666C1.64923 4.10256 0.820511 4.93046 0.820511 5.94872V12.9231C0.820511 14.9071 1.91343 16 3.89743 16H12.1026C14.0866 16 15.1795 14.9071 15.1795 12.9231V5.94872C15.1795 4.93046 14.3508 4.10256 13.3333 4.10256ZM5.74359 3.48718C5.74359 2.24246 6.75528 1.23077 8 1.23077C9.24472 1.23077 10.2564 2.24246 10.2564 3.48718V4.10256H5.74359V3.48718ZM13.9487 12.9231C13.9487 14.217 13.3965 14.7692 12.1026 14.7692H3.89743C2.60349 14.7692 2.05128 14.217 2.05128 12.9231V5.94872C2.05128 5.60903 2.32779 5.33333 2.66666 5.33333H4.51282V7.17949C4.51282 7.51918 4.78851 7.79487 5.1282 7.79487C5.4679 7.79487 5.74359 7.51918 5.74359 7.17949V5.33333H10.2564V7.17949C10.2564 7.51918 10.5321 7.79487 10.8718 7.79487C11.2115 7.79487 11.4872 7.51918 11.4872 7.17949V5.33333H13.3333C13.6722 5.33333 13.9487 5.60903 13.9487 5.94872V12.9231Z"
                                                            fill="#00A8E8" />
                                                    </svg>
                                                    <span class="button-text">{{ button_cart }}</span>
                                                </button>
                                            {% else %}
                                                <button type="button" aria-label="To cart" class="button button-outline button-outline-primary br-8 fsz-12 ds-module-cart-btn ms-0 position-absolute bottom-0 end-0">
                                                <svg width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg">
                                                    <path
                                                        d="M13.3333 4.10256H11.4872V3.48718C11.4872 1.5639 9.92328 0 8 0C6.07672 0 4.51282 1.5639 4.51282 3.48718V4.10256H2.66666C1.64923 4.10256 0.820511 4.93046 0.820511 5.94872V12.9231C0.820511 14.9071 1.91343 16 3.89743 16H12.1026C14.0866 16 15.1795 14.9071 15.1795 12.9231V5.94872C15.1795 4.93046 14.3508 4.10256 13.3333 4.10256ZM5.74359 3.48718C5.74359 2.24246 6.75528 1.23077 8 1.23077C9.24472 1.23077 10.2564 2.24246 10.2564 3.48718V4.10256H5.74359V3.48718ZM13.9487 12.9231C13.9487 14.217 13.3965 14.7692 12.1026 14.7692H3.89743C2.60349 14.7692 2.05128 14.217 2.05128 12.9231V5.94872C2.05128 5.60903 2.32779 5.33333 2.66666 5.33333H4.51282V7.17949C4.51282 7.51918 4.78851 7.79487 5.1282 7.79487C5.4679 7.79487 5.74359 7.51918 5.74359 7.17949V5.33333H10.2564V7.17949C10.2564 7.51918 10.5321 7.79487 10.8718 7.79487C11.2115 7.79487 11.4872 7.51918 11.4872 7.17949V5.33333H13.3333C13.6722 5.33333 13.9487 5.60903 13.9487 5.94872V12.9231Z"
                                                        fill="#00A8E8" />
                                                </svg>
                                                <span class="button-text">{{ button_cart }}</span>
                                            </button>
                                            {% endif %}
                                        {% endif %}
                                        <input type="hidden" name="product_id" value="{{ product.product_id }}" />
                                    </div>
                                </div>
                            </div>
                        </div>
                    {% endfor %}
                </div>
                <div class="swiper-button-prev swiper-button-prev-{{ module }} button button-secondary button-with-icon br-4 overflow-hidden d-flex align-items-center justify-content-center py-3 px-1">
					<svg class="me-0" xmlns="http://www.w3.org/2000/svg" width="4" height="6" viewBox="0 0 4 6" fill="none">
						<path d="M0.263207 2.99938C0.263207 2.87814 0.309287 2.75688 0.402114 2.66468L2.92803 0.138767C3.11305 -0.0462558 3.41303 -0.0462558 3.59805 0.138767C3.78307 0.323791 3.78307 0.623765 3.59805 0.808788L1.40684 3L3.59805 5.19121C3.78307 5.37624 3.78307 5.67621 3.59805 5.86123C3.41303 6.04626 3.11305 6.04626 2.92803 5.86123L0.402114 3.33532C0.309287 3.24186 0.263207 3.12063 0.263207 2.99938Z" fill="#003459"/>
					</svg>
				</div>
				<div class="swiper-button-next swiper-button-next-{{ module }} button button-secondary button-with-icon br-4 overflow-hidden d-flex align-items-center justify-content-center py-3 px-1">
					<svg class="me-0" xmlns="http://www.w3.org/2000/svg" width="4" height="6" viewBox="0 0 4 6" fill="none">
						<path d="M3.73679 3.00062C3.73679 3.12186 3.69071 3.24312 3.59789 3.33532L1.07197 5.86123C0.886949 6.04626 0.586974 6.04626 0.401951 5.86123C0.216928 5.67621 0.216928 5.37623 0.401951 5.19121L2.59316 3L0.401951 0.808788C0.216928 0.623765 0.216928 0.32379 0.401951 0.138767C0.586974 -0.0462565 0.886949 -0.0462565 1.07197 0.138767L3.59789 2.66468C3.69071 2.75814 3.73679 2.87937 3.73679 3.00062Z" fill="#003459"/>
					</svg>
				</div>
            </div>
            <script>
                window.addEventListener('DOMContentLoaded', () => {
                    var columnModulesSwiper = new Swiper('#ds-column-{{ module_name }}_{{ module }}', {
                        slidesPerView: 1,
                        spaceBetween: 0,
                        loop: true,
                        navigation: {
                            nextEl: '.swiper-button-next-{{ module }}',
                            prevEl: '.swiper-button-prev-{{ module }}',
                        },
                    });
                });
            </script>
        </div>
    {% else %}
        <div class="py-3 fw-500 dark-text fsz-20">
            {% if link is defined and link %}<a href="{{ link }}">{% endif %}
                {{ heading_title }}
            {% if link is defined and link %}</a>{% endif %}
        </div>
        <div id="ds-{{ module_name }}_{{ module }}" class="row g-2 g-md-3 mb-3 ds-module"{% if show_type == '' %} data-type="split"{% endif %}{% if show_type == 'width-100' %} data-type="width100"{% endif %}{% if show_type == 'width-50' %} data-type="width50"{% endif %}{% if show_type == 'width-infinity' %} data-type="carousel"{% endif %}{% if show_type == 'width-minimal' %} data-type="minimal"{% endif %}>
            {% for product in products %}
            <div class="ds-module-item ds-module-col">
                <div class="content-block h-100 d-flex flex-column{% if product.oct_grayscale %} ds-no-stock{% endif %}">
                    <div class="ds-module-img d-flex flex-column">
                        <div class="ds-module-stickers d-flex flex-wrap">
                            {% if product.oct_stickers is not empty %}
                                {% for key, oct_sticker in product.oct_stickers %}
                                    {% if oct_sticker is not empty and oct_sticker is not iterable %}
                                        <div class="ds-module-sticker br-12 fw-500 ds-module-sticker-{{ key }}">
                                            {{ oct_sticker }}
                                        </div>
                                    {% endif %}
                                {% endfor %}
                            {% endif %}
                        </div>
                        <div class="ds-module-img-box position-relative">
                            <div class="ds-module-buttons position-absolute d-flex flex-column br-8">
                                {% if oct_popup_view_status %}
