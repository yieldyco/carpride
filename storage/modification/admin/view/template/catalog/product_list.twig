                        {% set i = 0 %}
                        {% set count_images = product.images|length %}
                        {% for image in product.images %}
                            {% if count_images <= 3 and i < 3 %}
                                <img src="{{ image.thumb }}" alt="{{ product.name }}" class="img-thumbnail" />&nbsp;
                            {% elseif count_images > 3 and i < 2 %}
                                <img src="{{ image.thumb }}" alt="{{ product.name }}" class="img-thumbnail" />&nbsp;
                            {% endif %}
                            {% set i = i + 1 %}
                        {% endfor %}
