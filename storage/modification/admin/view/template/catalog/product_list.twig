                </td>
            <!-- Admin Product Editor and extended Filter -->
            
                    <td class="text-left">{{ product.name }}</td>
                    <td class="text-left">{{ product.model }}</td>

            <!-- Admin Product Editor and extended Filter -->
                {% if ape_f_license %}
                <td class="text-left">{{ product.sku }}</td>
