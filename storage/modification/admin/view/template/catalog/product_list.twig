                                <span class="category-level-down">↴</span>&nbsp;{{ category.name }}<br>
                            {% endif %}
                        {% else %}
                            {% if category.category_id in product.category %}
                                <span class="category-level-down">↴</span>&nbsp;{{ category.name }}<br>
                            {% endif %}
                        {% endif %}
                    {% endfor %}
                </td>
                {% endif %}
