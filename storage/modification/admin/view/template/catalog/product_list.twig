                    <label class="hidden-md hidden-sm hidden-xs" style="color:#dd3520;float:left;text-align:right;margin-right:15px;width:230px;">{{ help_enable_disable }}:</label>
                    <button onclick="if (confirm('{{ text_confirm }}')) { $(this).attr('data-click', 'true'); $('#button-filter').click() } else { false; }" type="button" id="button-status-enable-all" data-toggle="tooltip" title="{{ help_enable_disable }}." class="btn btn-success"><i class="fa fa-power-off fa-rotate-180"></i> <span class="hidden-xs">{{ button_enable_all }}</span></button>
                    <button onclick="if (confirm('{{ text_confirm }}')) { $(this).attr('data-click', 'true'); $('#button-filter').click() } else { false; }" type="button" id="button-status-disable-all" data-toggle="tooltip" title="{{ help_enable_disable }}." class="btn btn-danger"><i class="fa fa-power-off"></i> <span class="hidden-xs">{{ button_disable_all }}</span></button>
