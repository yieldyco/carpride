                                display: block;
                                {% if logo_width is defined and logo_width %}width: {{ logo_width }}px;{% endif %}
                                {% if logo_height is defined and logo_height %}height: {{ logo_height }}px;{% endif %}
                                background-repeat: no-repeat;
                                background-size: contain;
                                background-position: center;
                            }

