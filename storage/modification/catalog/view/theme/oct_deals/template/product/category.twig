{{ header }}
    <div id="product-category" class="container-fluid container-xl flex-grow-1">
    <nav aria-label="breadcrumb">
        <ul class="breadcrumb ds-breadcrumb fsz-12">
        {% for breadcrumb in breadcrumbs %}
            {% if loop.last %}
                <li class="breadcrumb-item ds-breadcrumb-item">{{ breadcrumb.text }}</li>
            {% else %}
                <li class="breadcrumb-item ds-breadcrumb-item"><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
            {% endif %}
        {% endfor %}
        </ul>
    </nav>
    {% if oct_deals_data.micro is defined and oct_deals_data.micro %}
        <script type="application/ld+json">
        {
            "@context": "https://schema.org",
            "@type": "BreadcrumbList",
            "itemListElement":
            [
                {% for breadcrumb in breadcrumbs %}
                    {% if loop.first %}
                    {% else %}
                    {
                        "@type": "ListItem",
                        "position": {{ loop.index -1 }},
                        "item":
                        {
                            "@id": "{{ breadcrumb.href }}",
                            "name": "{{ breadcrumb.text }}"
                        }
                    }{% if not loop.last %},{% endif %}
                    {% endif %}
                {% endfor %}
            ]
        }
        </script>
    {% endif %}
    <main>
        <div class="row">
            <div class="col-12 ds-page-title pb-3">
                <h1>{{ heading_title }}</h1>
            </div>
        </div>
        <div class="content-top-box">{{ content_top }}</div>

<div id="category-mobile-search-anchor" class="d-xl-none w-100 mb-3"></div>

        <div class="row g-3 ds-category">

            <script>
                document.addEventListener('DOMContentLoaded', function () {
                    const DESKTOP_QUERY = window.matchMedia('(min-width: 1200px)'); // xl и шире
                    const colLeft  = document.getElementById('column-left');
                    const mobileAnchor = document.getElementById('category-mobile-search-anchor');
                    if (!colLeft || !mobileAnchor) return;

                    let box = document.querySelector('.ds-category-scoped-search');
                    if (!box) {
                        box = document.createElement('div');
                        box.className = 'content-block p-3 mb-3 ds-category-scoped-search';
                        box.innerHTML =
                            '<div class="fw-700 dark-text fsz-14 mb-2" style="letter-spacing:.2px;">{{ form_search_cat }}</div>' +
                            '<form action="product/search" method="get" class="d-flex gap-2">' +
                            '<input type="hidden" name="route" value="product/search">' +
                            '<input type="hidden" name="category_id" value="{{ category_id }}">' +
                            '<input type="hidden" name="sub_category" value="false">' +
                            '<input type="text" name="search" class="form-control br-7" placeholder="{{ form_search_cat_i }}">' +
                            '<button type="submit" class="button button-primary br-7 px-3">' +
                            '<span class="button-text">{{ button_search is defined ? button_search : "Знайти" }}</span>' +
                            '</button>' +
                            '</form>' +
                            '<div class="light-text fsz-11 mt-2">{{ form_search_cat_f }}</div>';
                        document.body.appendChild(box);
                    }

                    function placeBox() {
                        if (DESKTOP_QUERY.matches) {
                            // Десктоп: в левую колонку, сверху
                            if (colLeft.firstElementChild !== box) colLeft.prepend(box);
                        } else {
                            // Мобилка: под заголовок/над сортировкой
                            if (mobileAnchor.nextElementSibling !== box) mobileAnchor.after(box);
                        }
                    }

                    placeBox();

                    DESKTOP_QUERY.addEventListener('change', placeBox);
                });
            </script>

            <style>
                .ds-category-scoped-search .form-control { height: 40px; }
                .ds-category-scoped-search .button { height: 40px; }

                @media (max-width: 1199.98px) {
                    #category-mobile-search-anchor + .ds-category-scoped-search { margin-top: .25rem; }
                }
            </style>

            {{ column_left }}
            {% if column_left and column_right %}
                {% set class = 'col-xl-6 is-cols' %}
            {% elseif column_left or column_right %}
                {% set class = 'col-xl-9' %}
            {% else %}
                {% set class = 'col-xl-12 no-col' %}
            {% endif %}
            <div id="content" class="{{ class }}">
                {% if oct_deals_data.category_desc_position is defined and oct_deals_data.category_desc_position == 'top' %}
                    {% if thumb or description %}
                        <div class="ds-category-description content-block d-flex flex-column flex-md-row mb-3">
                            {% if thumb %}
                                <div class="ds-category-description-img">
                                    <img src="{{ thumb }}" class="d-block mx-auto" alt="{{ heading_title }}" title="{{ heading_title }}" width="{{ cat_image_width }}" height="{{ cat_image_height }}" />
                                </div>
                            {% endif %}
                            {% if description %}
                                <div id="ds-category-description"{% if thumb %} class="pt-3 pt-md-0 ps-md-3"{% endif %}>
                                    <div class="ds-category-description-text secondary-text fw-300">
                                        {{ description }}
                                        {% if categories_page is defined and categories_page is not empty %}
                                            <div class="ds-category-categories_page-text mt-3">
                                                <ul class="category-landing-links list-unstyled ps-0">
                                                {% for oct_page in categories_page %}
                                                    {% if oct_page.page_links is not empty %}
                                                    <li class="category-landing-links-item">
                                                        <span class="category-landing-links-item-title">{{ oct_page.page_group[language_id] }}</span>
                                                        {% for link in oct_page.page_links %}
                                                        <a href="{{ link[language_id].link }}" class="blue-link d-flex-inline align-items-center" title="{{ link[language_id].title }}">{{ link[language_id].title }}</a>
                                                        {% endfor %}
                                                    </li>
                                                    {% endif %}
                                                {% endfor %}
                                                </ul>
                                            </div>
                                        {% endif %}
                                    </div>
                                    <!-- if button enabled -->
                                    {% if description|length > 700 and (categories or products) and oct_deals_data.category_desc_up is not defined %}
                                        <div class="mt-3">
                                            <span class="ds-category-description-text-toggle text-decoration-underline fsz-14 fw-400 dark-text">
                                                {{ oct_expand }}
                                                <svg class="ms-1" xmlns="http://www.w3.org/2000/svg" width="6" height="4" viewBox="0 0 6 4" fill="none">
                                                    <path
                                                        d="M2.99938 3.85142C2.87814 3.85142 2.75688 3.80534 2.66468 3.71251L0.138767 1.1866C-0.0462558 1.00157 -0.0462558 0.701598 0.138767 0.516575C0.323791 0.331552 0.623765 0.331552 0.808788 0.516575L3 2.70779L5.19121 0.516575C5.37624 0.331552 5.67621 0.331552 5.86123 0.516575C6.04626 0.701598 6.04626 1.00157 5.86123 1.1866L3.33532 3.71251C3.24186 3.80534 3.12063 3.85142 2.99938 3.85142Z"
                                                        fill="#00171F" />
                                                </svg>
                                            </span>
                                            <script>
                                                document.addEventListener('DOMContentLoaded', function () {
                                                    toggleCategoryDescription();
                                                });
                                            </script>
                                        </div>
                                    {% else %}
                                        <style>
                                            .ds-category-description-text {
                                                max-height: none !important;
                                            }
                                        </style>
                                    {% endif %}
                                    <!-- end if -->
                                </div>
                            {% endif %}
                        </div>
                    {% endif %}
                {% endif %}
                {% if categories %}
                    <div class="ds-subcategories row mb-3 g-2">
                        {% for category in categories %}
                            <div class="ds-subcategories-item">
                                <a href="{{ category.href }}" class="content-block d-flex flex-column align-items-center p-2 h-100{% if not oct_deals_data.category_subcategory_photo  %} justify-content-center{% endif %}">
                                    {% if oct_deals_data.category_subcategory_photo  %}
                                        <img src="{{ category.image }}" alt="" title="{{ category.name }}" width="{{ category.width }}" height="{{ category.height }}">
                                        <span class="ds-subcategories-title dark-text fw-500 fsz-12 lh-sm">{{ category.name }}</span>
                                    {% else %}
                                        <span class="ds-subcategories-title dark-text fw-500 fsz-14 align-self-center">{{ category.name }}</span>
                                    {% endif %}
                                </a>
                            </div>
                        {% endfor %}
                    </div>
                {% endif %}
                <!-- Filter button here -->
                {# <div class="d-xl-none ds-filter-btn sticky-top mb-3">
                    <button type="button" class="button button-primary br-7">
