                            </div>
                            <script>
                                window.addEventListener('DOMContentLoaded', () => {
                                    switchCategoryDisplay();
                                });
                            </script>
                        </div>
                    {% endif %}
                    <div class="row g-2 g-md-3 ds-module ds-category-products"
                        {% if oct_deals_data.category_show_type == '' %} data-type="split"{% endif %}
                        {% if oct_deals_data.category_show_type == 'width-100' %} data-type="width100"{% endif %}
                        {% if oct_deals_data.category_show_type == 'width-50' %} data-type="width50"{% endif %}
                        {% if oct_deals_data.category_show_type == 'width-minimal' %} data-type="minimal"{% endif %}>
                        {% for product in products %}
                            <div class="ds-module-item product-layout product-grid col-sm-6 col-md-4 col-lg-3 ds-module-col" data-pid="{{ product.product_id }}">
                                <div class="content-block h-100 d-flex flex-column position-relative{% if product.oct_grayscale %} ds-no-stock{% endif %}">
                                    <div class="ds-module-img d-flex flex-column">
                                        <div class="ds-module-stickers d-flex flex-wrap">
                                            {% if product.oct_stickers is not empty %}
                                                {% for key, oct_sticker in product.oct_stickers %}
                                                    {% if oct_sticker is not empty and oct_sticker is not iterable %}
                                                        <div class="ds-module-sticker br-12 fw-500 ds-module-sticker-{{ key }}">
                                                            {{ oct_sticker }}
                                                        </div>
                                                    {% endif %}
                                                {% endfor %}
                                            {% endif %}
                                        </div>
                                        <div class="ds-module-img-box position-relative">
                                            <div class="ds-module-buttons position-absolute d-flex flex-column br-8">
